  "main": "dist/index.js",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.17",
    "@types/lodash": "^4.14.165",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "typescript": "^4.1.2",
    "uglify-es": "^3.3.9"
  },
  "peerDependencies": {
    "@types/react": ">=18.2.0",
    "react": "^18.2.0"
  },
  "dependencies": {
